{
  "name": "@niktekio/query-packages",
  "version": "0.0.4",
  "description": "Queries stats from npm and github for certain packages",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "bun src/query.ts",
    "gen": "drizzle-kit generate:sqlite --out ./src/data/migrations --schema ./src/data/schema/schema.ts",
    "migrate": "tsx src/data/migrate.ts",
    "ingest": "tsx src/ingest.ts",
    "build": "tsup src/*.ts --format cjs,esm --dts"
  },
  "keywords": [],
  "author": "niktek",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@octokit/graphql": "^7.0.1",
    "drizzle-kit": "^0.19.13"
  },
  "dependencies": {
    "@orama/orama": "^1.2.3",
    "dotenv": "^16.0.3",
    "drizzle-orm": "^0.28.6",
    "postgres": "^3.4.0"
  }
}
